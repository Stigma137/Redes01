package client;

import java.io.IOException;
import java.net.Socket;

public class Client {

	public static void main(String[] args) {
		try {
			Socket socket = new Socket("127.0.0.1", 5000);
			System.out.println("Client connected");
			while(true) {}
		} catch (IOException e) {
			e.printStackTrace();
		}
		
	}

}
